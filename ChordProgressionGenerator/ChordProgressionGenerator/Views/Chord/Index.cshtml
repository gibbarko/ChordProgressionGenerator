@model List<ChordProgressionGenerator.Models.Chord>

<h1>All Chords in Database</h1>

<div>
     <ul>
        @foreach (Chord chord in Model)
        {
            <li>
                <p>Chord Name: @chord.CHORD_ROOT@chord.CHORD_TYPE || Chord Structure: @chord.CHORD_STRUCTURE || Finger Positioning: @chord.FINGER_POSITIONS</p>
            </li>
        }
     </ul>
</div>